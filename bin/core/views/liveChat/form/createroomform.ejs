<form id="add_room" class="add_room" method="post" action="/livechat/room">
  <header><h3>채팅방 추가</h3></header>
  <div>
    <label for="input_roomname">채팅방 이름 입력</label>
    <input
      type="text"
      name="room"
      id="input_roomname"
      class="input_roomname"
      placeholder="채팅방 이름을 입력하세요"
      required
    />
  </div>
  <button type="submit">방 추가</button>
</form>
